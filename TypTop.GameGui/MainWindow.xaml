<Window x:Class="TypTop.GameGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TypTop.GameGui"
        xmlns:gameWindow="clr-namespace:TypTop.GameWindow;assembly=TypTop.GameWindow"
        xmlns:visualKeyboard="clr-namespace:TypTop.VisualKeyboard;assembly=TypTop.VisualKeyboard"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1370" WindowStartupLocation="CenterScreen"
        Background="Black">
    <Viewbox Stretch="Uniform" Grid.Row="0">
        <gameWindow:GameWindow ClipToBounds="True" Height="1080" Width="1920" x:Name="GameWindow" />
    </Viewbox>
</Window>
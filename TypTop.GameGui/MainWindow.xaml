<Window x:Class="TypTop.GameGui.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TypTop.GameGui"
        xmlns:gameWindow="clr-namespace:TypTop.GameWindow;assembly=TypTop.GameWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1000"
        Background="Black">
    <Grid>
        <Viewbox Stretch="Uniform">
            <gameWindow:GameWindow Height="1080" Width="1920" x:Name="GameWindow"></gameWindow:GameWindow>
        </Viewbox>
    </Grid>
</Window>

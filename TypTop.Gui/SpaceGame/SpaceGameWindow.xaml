<Window x:Class="TypTop.Gui.SpaceGame.SpaceGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TypTop.Gui.SpaceGame"
        mc:Ignorable="d"
        Title="SpaceGameWindow" Height="450" Width="800">
    <Grid>
        <Canvas x:Name="SpaceGameCanvas" Background='#112'/>
        <Line x:Name="Line" X1="0" X2="800" Y1="350" Y2="350" Stroke="Red" StrokeThickness="5"/>
    </Grid>
</Window>
